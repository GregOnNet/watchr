<?xml version="1.0"
      encoding="utf-8"?>

<configuration>
  <appSettings>
    <!-- Minimized, Normal, Maximized. -->
    <add key="initial-state"
         value="<%= ('Minimized' if configatron.env =~ /production/) or 'Normal' %>" />

    <add key="hub-url"
         value="@configatron.deployment.canoncial_url@" />

    <add key="group-id"
         value="" />

    <add key="screen-logs"
         value="@configatron.client.shell_transscripts@" />
  </appSettings>

  <startup>
    <supportedRuntime version="v4.0"
                      sku=".NETFramework,Version=v4.5.1" />
  </startup>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json"
                          publicKeyToken="30ad4fe6b2a6aeed"
                          culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0"
                         newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
